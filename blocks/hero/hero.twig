#}  

<section class="hero">
	<div class="swiper mySwiper">
		<div class="swiper-wrapper">
			{% for field in fields.images %}
				<div class="swiper-slide">
					<img style="height:100vh;width:100vw;object-fit:cover;" src="{{ field.image.url }}" />
				</div>
			{% endfor %}
		</div>
		<!-- <div class="swiper-pagination"></div> -->
	</div>
</section> 

